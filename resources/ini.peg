INIGrammar:
  INI < (Section / Comment)* :eoi

  Comment <~ :"#" (!eol .)* (:eol/:eoi)

  Section < Header :Comment? (Decl :Comment? / :Comment)*

  Header < :"[" identifier (:"." identifier)? :"]"

  Decl < identifier :"=" Literal

  Literal < ^Terminals.String / ^Terminals.Number / Terminals.Boolean / ^Terminals.Null
